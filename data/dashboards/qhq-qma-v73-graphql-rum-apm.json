{
  "dashboard_id": "",
  "title": "GraphQL Monitoring RUM + APM",
  "fetched_at": "2026-02-16",
  "metrics": {
    "rum": [
      "@resource.duration",
      "@usr.email",
      "@usr.id",
      "@view.url_host",
      "@context.apollo.client.operationName",
      "@context.error.type",
      "@context.apollo.client.error.extensions.code"
    ],
    "apm_trace": [
      "trace.graphql.server.request.duration",
      "trace.graphql.server.request.hits",
      "trace.graphql.server.request.errors"
    ],
    "nodejs_runtime": [
      "runtime.node.event_loop.delay.avg",
      "runtime.node.event_loop.delay.max",
      "runtime.node.event_loop.delay.min",
      "runtime.node.gc.pause.avg",
      "runtime.node.gc.pause.max",
      "runtime.node.mem.heap_used",
      "runtime.node.mem.heap_total",
      "runtime.node.mem.rss",
      "runtime.node.process.active_handles",
      "runtime.node.process.active_requests"
    ],
    "custom_graphql": [
      "graphql.request.size",
      "graphql.response.size",
      "graphql.query.depth",
      "graphql.query.complexity",
      "graphql.cache.hit",
      "graphql.cache.miss",
      "graphql.dataloader.batch_size"
    ],
    "redis": [
      "redis.keyspace_hits",
      "redis.keyspace_misses"
    ],
    "span_based": {
      "operation_names": ["graphql.parse", "graphql.validate", "graphql.execute", "graphql.resolve"],
      "span_types": ["sql", "redis", "http"],
      "facets": ["@db.system", "@http.url_details.host"]
    }
  }
}
